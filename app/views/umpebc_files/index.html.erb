<p id="notice"><%= notice %></p>
<div id="maincontent">
  <div class="container">
    <div class="row">
      <h1>UMPEBC Files</h1>
    </div>
    <div class="row">
      <div class="col">Name</div>
      <div class="col">Checksum</div>
      <div class="col-3">&nbsp;</div>
    </div>
    <div class="row">&nbsp;</div>
    <% @umpebc_files.each do |umpebc_file| %>
      <div class="row">
        <div class="col"><%= umpebc_file.name %></div>
        <div class="col"><%= umpebc_file.checksum %></div>
        <div class="col-3">
          <div class="row">
            <div class="col"><%= link_to 'Destroy', umpebc_file, method: :delete, data: { confirm: 'Are you sure?' } %></div>
          </div>
        </div>
      </div>
    <% end %>
    <div class="row">
      <div class="col">&nbsp<%= paginate @umpebc_files %></div>
    </div>
  </div>
</div>
